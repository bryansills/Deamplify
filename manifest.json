{
  "manifest_version": 2,
  "name": "Deamplify",
  "version": "0.1",
  "description": "Adds a red border to all webpages matching mozilla.org.",
  "icons": {
    "48": "icons/deamplify.svg",
    "96": "icons/deamplify.svg"
  },
  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["borderify.js"]
    }
  ]
}
